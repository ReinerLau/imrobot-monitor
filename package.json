{
  "name": "imrobot-monitor",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "schema:build": "pnpm -F @imrobot/schema build",
    "db:init": "pnpm schema:build && pnpm -F @imrobot/db run init",
    "docker:start": "docker-compose up -d",
    "dev:test": "pnpm -F test-example dev",
    "dev:client": "pnpm -F client dev",
    "dev:serve": "pnpm -F api run start:dev",
    "prod:serve": "pnpm -F api build && pnpm -F api start:prod",
    "prod:client": "pnpm -F client build && pnpm -F client preview",
    "prod:test": "pnpm -F test-example build-only && pnpm -F test-example preview",
    "commit": "cz",
    "prepare": "husky",
    "changeset": "changeset"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.5",
    "husky": "^9.0.11",
    "tsup": "^8.0.2",
    "typescript": "~5.3.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
